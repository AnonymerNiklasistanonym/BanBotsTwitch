{
  "name": "banbots",
  "version": "1.0.1",
  "main": "./dist/main.js",
  "scripts": {
    "compile": "tsc",
    "start": "node .",
    "pkg": "pkg ."
  },
  "bin": {
    "banbots": "./dist/main.js"
  },
  "pkg": {
    "assets": [
      "./node_modules/axios/dist/node/axios.cjs"
