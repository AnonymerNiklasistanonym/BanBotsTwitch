    ],
    "targets": [
      "node20-linux-arm64",
      "node20-linux-x64",
      "node20-windows-x64"
    ]
  },
  "author": "AnonymerNiklasistanonym",
  "license": "MIT",
  "description": "Automatically ban recent followers or a list of users.",
  "dependencies": {
    "@twurple/api": "^7.1.0",
    "@twurple/auth": "^7.1.0",
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^5.11.5",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2"
  }
}
